              {{ block.settings.second_acc_head }}
              <span class="icon"
                ><img src="https://cdn.shopify.com/s/files/1/0937/6366/6242/files/arrow_2.png?v=1747499733"
              ></span>
            </div>
            <div class="product_accordion-content">
              {{ product.metafields.custom.most_common_questions | metafield_tag }}
            </div>
          </div>
        {% endif %}

        {% if product.metafields.custom.processing_shipping != blank %}
          <div class="product_accordion-item">
            <div class="product_accordion-header" onclick="toggleProductAccordion(this)">
              {{ block.settings.third_acc_head }}
              <span class="icon"
                ><img src="https://cdn.shopify.com/s/files/1/0937/6366/6242/files/arrow_2.png?v=1747499733"
              ></span>
            </div>
            <div class="product_accordion-content">
              {{ product.metafields.custom.processing_shipping | metafield_tag }}
            </div>
          </div>
        {% endif %}
      </div>

      <script>
        function toggleProductAccordion(header) {
          const content = header.nextElementSibling;
          const isOpen = content.classList.contains("open");

          document.querySelectorAll(".product_accordion-content").forEach(el => {
            el.classList.remove("open");
            el.style.maxHeight = null;
          });

          if (!isOpen) {
            content.classList.add("open");
            content.style.maxHeight = content.scrollHeight + "px";
          }
        }
      </script>

